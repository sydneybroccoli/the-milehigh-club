<%= simple_form_for [@aircraft, @booking] do |f| %>
  <%= f.input :booking_type,
  placeholder: 'RENTAL',
  readonly: true %>
  <%= f.input :start_date,
    as: :string,
    type: :text %>
  <%= f.input :end_date,
    as: :string,
    type: :text %>
  <%= f.submit "Book now", class: "nav-custom grey" %>
<% end %>
